<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <package android:name="com.wfql.server" />

        <provider android:authorities="com.wfql.server.provider.LoginProvider" />
    </queries>

    <application
        android:name=".activity.clientApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Leaveclient"
        tools:targetApi="31">
        <activity
            android:name=".activity.AboutActivity"
            android:exported="false" />
        <activity
            android:name=".activity.SettingActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ChangePwdActivity"
            android:exported="false" />
        <activity
            android:name=".activity.WelcomeActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CheckActivity"
            android:exported="false" />
        <activity
            android:name=".activity.AskLeaveActivity"
            android:exported="false" />
        <activity
            android:name=".activity.AdmActivity"
            android:exported="false" />
        <activity
            android:name=".activity.VerifyLoginActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ForgetPwdActivity"
            android:exported="false" />
        <activity
            android:name=".activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".activity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>